<view class='content'>
  <view class='user'>
    <image src='{{iconSrc}}'></image>
    <text class='username'>{{username}}</text>
  </view>

  <view class='titlt-view'>{{text}}</view>
  <video src="{{video_uri}}" controls objectFit="contain" custom-cache="{{false}}"  autoplay='true'></video>
  
  <view class='saw'>
    <image class='eye' src='../../images/eye.png'></image>
    <text class='eyenums'>{{eyenums}}</text>
  </view>



  <view class='love-hate'>
    <view class='love-view' bindtap='loveClick'>
      <image class='love' src='{{loveSrc}}'></image>
      <text>{{love}}</text>
    </view>
    <view class='hate-view' bindtap='hateClick'>
      <image class='hate' src='{{hateSrc}}'></image>
      <text>{{hate}}</text>
    </view>
  </view>

  <view class='bot-view'>
    <view>END</view>
    <image class='bot' src='../../images/bot.jpg' mode='aspectFill'></image>

  </view>
  <view class='pick' bindtap='pickshipin'>biu</view>
</view>